{
  "extends": [
    "eslint:recommended",
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "skipBlankLines": 0,
    "no-trailing-spaces": 0,
    "object-curly-spacing": 0,
    "no-shadow": "warn",
    "no-underscore-dangle": 0,
    "max-len": 0
  }
}
